<% include ../partials/header.ejs  %>
<h1>New Parent</h1>
<form method="post" action="/parents">
<div class="form-group">
  <label for="InputParentName">New Parent's Name</label>
  <input type="text" class="form-control" id="name" name="ParentName[name]" placeholder="Type Parent Name">
</div>
<div class="form-group">
  <label for="InputParentEmail">New Parent's Email</label>
  <input type="text" class="form-control" id="email" name="ParentEmail[email]" placeholder="Type Parent Email">
</div>
<div class="form-group">
  <label for="InputParentAge">New Parent's Age</label>
  <input type="text" class="form-control" id="age" name="ParentAge[age]" placeholder="Type Parent Age">
</div>
<button type="submit" class="btn btn-default">Submit</button>
</form>
<% include ../partials/footer.ejs  %>
